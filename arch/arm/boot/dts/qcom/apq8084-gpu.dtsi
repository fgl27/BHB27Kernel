&soc {
	msm_gpu: qcom,kgsl-3d0@fdb00000 {
		label = "kgsl-3d0";
		compatible = "qcom,kgsl-3d0", "qcom,kgsl-3d";
		reg = <0xfdb00000 0x1B000
		       0xfdb20000 0x10000>;
		reg-names = "kgsl_3d0_reg_memory" , "kgsl_3d0_shader_memory";
		interrupts = <0 33 0>;
		interrupt-names = "kgsl_3d0_irq";
		qcom,id = <0>;

		qcom,chipid = <0x04020000>;

		qcom,initial-pwrlevel = <8>;

		qcom,idle-timeout = <80>; // 80ms
		qcom,strtstp-sleepwake;

		/*
		 * Clocks = KGSL_CLK_CORE | KGSL_CLK_IFACE |
		 * KGSL_CLK_RBBMTIMER
		 */
		qcom,clk-map = <0x00000086>;

		qcom,pm-qos-active-latency = <501>;
		qcom,pm-qos-wakeup-latency = <101>;


		/* Bus Scale Settings */
		qcom,bus-control;
		qcom,msm-bus,name = "grp3d";
		qcom,msm-bus,num-cases = <25>;
		qcom,msm-bus,num-paths = <2>;
		qcom,msm-bus,vectors-KBps =
				<26 512 0 0>, <89 662 0 0>,     // 0  gpu=25 bus=0 * used

				<26 512 0 2400000>,  <89 662 0 2656000>,     // 1 gpu=166 bus=150 *
				<26 512 0 3200000>,  <89 662 0 2656000>,     // 2 gpu=166 bus=200

				<26 512 0 2400000>,  <89 662 0 3200000>,     // 3 gpu=200 bus=150
				<26 512 0 3200000>,  <89 662 0 3200000>,     // 4 gpu=200 bus=200
				<26 512 0 4915200>,  <89 662 0 3200000>,     // 5 gpu=200 bus=307.2 *
				<26 512 0 6144000>,  <89 662 0 3200000>,     // 6 gpu=200 bus=384

				<26 512 0 3200000>,  <89 662 0 4256000>,     // 7 gpu=266 bus=200
				<26 512 0 4915200>,  <89 662 0 4256000>,     // 8 gpu=266 bus=307.2
				<26 512 0 6144000>,  <89 662 0 4256000>,     // 9 gpu=266 bus=384 *
				<26 512 0 8448000>,  <89 662 0 4256000>,     // 10 gpu=266 bus=528

				<26 512 0 6144000>,  <89 662 0 5600000>,     // 11 gpu=300 bus=384 *
				<26 512 0 8448000>,  <89 662 0 5600000>,     // 12 gpu=350 bus=528 *
				<26 512 0 10598400>, <89 662 0 5600000>,     // 13 gpu=350 bus=662.4

				<26 512 0 8448000>,  <89 662 0 8000000>,     // 14 gpu=500 bus=528 *
				<26 512 0 10598400>, <89 662 0 8000000>,     // 15 gpu=500 bus=662.4 *
				<26 512 0 12748800>, <89 662 0 8000000>,     // 16 gpu=500 bus=796.8

				<26 512 0 10598400>, <89 662 0 9600000>,     // 17 gpu=600 bus=662.4
				<26 512 0 12748800>, <89 662 0 9600000>,     // 18 gpu=600 bus=796.8 *

				<26 512 0 12748800>, <89 662 0 11200000>,    // 19 gpu=700 bus=796.8
				<26 512 0 14400000>, <89 662 0 11200000>,    // 20 gpu=700 bus=900 *

				<26 512 0 12748800>, <89 662 0 12000000>,    // 21 gpu=750 bus=796.8
				<26 512 0 14400000>, <89 662 0 12000000>,    // 22 gpu=750 bus=900 *

				<26 512 0 14400000>, <89 662 0 12800000>,    // 23 gpu=800 bus=900
				<26 512 0 15200000>, <89 662 0 12800000>;    // 24 gpu=800 bus=950				

		/* GDSC oxili regulators */
		vddcx-supply = <&gdsc_oxili_cx>;
		vdd-supply = <&gdsc_oxili_gx>;

		/* IOMMU Data */
		iommu = <&kgsl_iommu>;

		/* Trace bus */
		coresight-id = <67>;
		coresight-name = "coresight-gfx";
		coresight-nr-inports = <0>;
		coresight-outports = <0>;
		coresight-child-list = <&funnel_in0>;
		coresight-child-ports = <4>;

		/* Power levels */
		qcom,gpu-pwrlevels {
			#address-cells = <1>;
			#size-cells = <0>;

			compatible = "qcom,gpu-pwrlevels";

			qcom,gpu-pwrlevel@0 {
				reg = <0>;
				qcom,gpu-freq = <800000000>;
				qcom,bus-freq = <24>;
			};

			qcom,gpu-pwrlevel@1 {
				reg = <1>;
				qcom,gpu-freq = <700000000>;
				qcom,bus-freq = <20>;
			};

			qcom,gpu-pwrlevel@2 {
				reg = <2>;
				qcom,gpu-freq = <600000000>;
				qcom,bus-freq = <18>;
			};

			qcom,gpu-pwrlevel@3 {
				reg = <3>;
				qcom,gpu-freq = <500000000>;
				qcom,bus-freq = <15>;
			};

			qcom,gpu-pwrlevel@4 {
				reg = <4>;
				qcom,gpu-freq = <400000000>;
				qcom,bus-freq = <14>;
			};

			qcom,gpu-pwrlevel@5 {
				reg = <5>;
				qcom,gpu-freq = <350000000>;
				qcom,bus-freq = <12>;
			};

			qcom,gpu-pwrlevel@6 {
				reg = <6>;
				qcom,gpu-freq = <300000000>;
				qcom,bus-freq = <11>;
			};

			qcom,gpu-pwrlevel@7 {
				reg = <7>;
				qcom,gpu-freq = <200000000>;
				qcom,bus-freq = <5>;
			};

			qcom,gpu-pwrlevel@8 {
				reg = <8>;
				qcom,gpu-freq = <100000000>;
				qcom,bus-freq = <1>;
			};

			qcom,gpu-pwrlevel@9 {
				reg = <9>;
				qcom,gpu-freq = <25000000>;
				qcom,bus-freq = <0>;
			};
		};

	};
};
